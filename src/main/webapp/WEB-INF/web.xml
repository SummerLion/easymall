<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>


    <!-- 配置全站乱码解决过滤器 -->
    <filter>
        <filter-name>EncodingFilter</filter-name>
        <filter-class>cn.tedu.filter.EncodingFilter</filter-class>
    </filter>
    <filter>
        <filter-name>AutoLoginFilter</filter-name>
        <filter-class>cn.tedu.filter.AutoLoginFilter</filter-class>
    </filter>
    <filter>
        <filter-name>PrivFilter</filter-name>
        <filter-class>cn.tedu.filter.PrivFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>AutoLoginFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

    <filter-mapping>
        <filter-name>PrivFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>


    <!-- 配置ServletContextListener监听器 -->
    <listener>
        <listener-class>cn.tedu.listener.MyServletContextListener</listener-class>
    </listener>
    <!-- 配置HttpSessionListener监听器 -->
    <listener>
        <listener-class>cn.tedu.listener.MyHttpSessionListener</listener-class>
